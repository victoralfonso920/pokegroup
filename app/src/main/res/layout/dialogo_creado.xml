<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/mainrr"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
    android:background="@color/transparent">

	<RelativeLayout
		android:id="@+id/LinearPrincipal"
		android:background="@drawable/dialogbg"
		android:layout_width="@dimen/_231sdp"
		android:layout_centerHorizontal="true"
		android:layout_centerVertical="true"
		android:layout_height="wrap_content" >
		<ImageView
			android:layout_width="@dimen/_40sdp"
			android:layout_height="@dimen/_40sdp"
			android:layout_marginTop="@dimen/_10sdp"
			android:scaleType="centerInside"
			android:layout_centerVertical="true"
			android:layout_alignParentEnd="true"
			android:src="@drawable/ic_pokeball"/>
	<LinearLayout
		android:gravity="center"
		android:layout_width="@dimen/_231sdp"
		android:layout_centerHorizontal="true"
		android:layout_centerVertical="true"
		android:orientation="vertical"
		android:layout_height="wrap_content">
		<TextView
			android:id="@+id/mensaje_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textColor="@color/text"
			android:layout_marginTop="@dimen/_10sdp"
			android:textSize="@dimen/_12sdp"
			android:layout_marginBottom="@dimen/_10sdp"
			android:text="Grupo pokemon"
			tools:ignore="SpUsage"/>
		<TextView
			android:id="@+id/name"
			android:layout_width="@dimen/_130sdp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textColor="@color/negro"
			android:layout_marginTop="@dimen/_10sdp"
			android:textSize="@dimen/_12sdp"
			android:text="Se ha creado el equipo\ndeseas revisarlo"
			tools:ignore="SpUsage"/>
		<LinearLayout
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="@dimen/_10sdp">
			<Button
				android:id="@+id/btnNp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="@android:color/transparent"
				android:textSize="@dimen/_13sdp"
				android:textColor="@color/redball"
				android:text="CANCELAR"
				tools:ignore="SpUsage"/>
			<Button
					android:id="@+id/btnSi"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@android:color/transparent"
					android:textSize="@dimen/_13sdp"
					android:textColor="@color/redball"
					android:text="ok"
					tools:ignore="SpUsage"/>
		</LinearLayout>
	</LinearLayout>

	</RelativeLayout>


</RelativeLayout>


